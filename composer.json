{
    "name": "coreb2c/yii2-auth",
    "description": "Flexible user registration and authentication module for Yii2",
    "keywords": ["yii2", "yii2-auth", "user management", "coreb2c"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/coreb2c/yii2-auth/issues?state=open",
        "source": "https://github.com/coreb2c/yii2-auth"
    },
    "authors": [
        {
            "name": "Abdullah Tulek",
            "email": "abdullah.tulek@coreb2c.com",
            "homepage": "http://www.coreb2c.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "^2.0.0",
        "yiisoft/yii2-swiftmailer": "^2.0.0",
        "yiisoft/yii2-authclient": "^2.1.0",
        "yiisoft/yii2-bootstrap": "^2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "^2.0.0",
        "codeception/specify": "^0.4.3",
        "codeception/verify": "^0.3.1"
    },
    "autoload": {
        "psr-4": {
            "coreb2c\\auth\\": ""
        }
    },
    "extra": {
        "bootstrap": "coreb2c\\auth\\Bootstrap",
         "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.0.x-dev"
        }
    }
}
